.slide-image {
  width:100%;
}
.main {
  display: flex;
  flex-wrap: wrap;
  justify-content: flex-start;
  border-top: 1rpx solid #f0f0f0;
  border-bottom: 1rpx solid #f0f0f0;
}
.itemButton {
  border: 1rpx solid #f0f0f0;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
  box-sizing: border-box;
  width: 250rpx;
  height: 250rpx;
  padding:30rpx;
}

